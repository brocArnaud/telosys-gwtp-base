<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<!-- Define resource for use style inside binding view -->
	<ui:with field="res" type="com.telosys.gwtp.base.client.resources.AppResources" />

	<g:HTMLPanel>
		<g:SimplePanel ui:field="headerPanel" />
		<g:SimplePanel ui:field="main" styleName="{res.style.mainWrapper}" />
		<g:SimplePanel ui:field="footerPanel" />
	</g:HTMLPanel>
</ui:UiBinder>
