<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	ui:baseMessagesInterface="com.telosys.gwtp.base.client.resources.i18n.Messages">

	<ui:with field="res" type="com.telosys.gwtp.base.client.resources.AppResources" />

	<g:HTMLPanel ui:field="container">
		<b:Navbar position="FIXED_TOP">
			<b:Container>
				<b:Row>
					<b:Column size="MD_11">
						<b:NavbarHeader>
							<b:Image ui:field="logo" type="ROUNDED" resource="{res.logo}" addStyleNames="{res.style.clickable}" />
							<b:NavbarCollapseButton dataTarget="#navbar-collapse" />
						</b:NavbarHeader>

						<b:NavbarCollapse b:id="navbar-collapse" ui:field="collapse">
							<b:NavbarNav>
								<b:AnchorListItem ui:field="author" text="Author">
									<ui:attribute name='text' key="layout.author" />
								</b:AnchorListItem>
								<b:AnchorListItem ui:field="badge" text="Badge">
									<ui:attribute name='text' key="layout.badge" />
								</b:AnchorListItem>
								<b:AnchorListItem ui:field="book" text="Book">
									<ui:attribute name='text' key="layout.book" />
								</b:AnchorListItem>
								<b:AnchorListItem ui:field="bookOrder" text="Book Order">
									<ui:attribute name='text' key="layout.book.order" />
								</b:AnchorListItem>
								<b:AnchorListItem ui:field="bookOrderItem" text="Book Order Item">
									<ui:attribute name='text' key="layout.book.order.item" />
								</b:AnchorListItem>
								<b:AnchorListItem ui:field="country" text="Country">
									<ui:attribute name='text' key="layout.country" />
								</b:AnchorListItem>
								<b:AnchorListItem ui:field="customer" text="Customer">
									<ui:attribute name='text' key="layout.customer" />
								</b:AnchorListItem>
								<b:AnchorListItem ui:field="employee" text="Employee">
									<ui:attribute name='text' key="layout.employee" />
								</b:AnchorListItem>
								<b:AnchorListItem ui:field="employeeGroup" text="Employee group">
									<ui:attribute name='text' key="layout.employee.group" />
								</b:AnchorListItem>
								<b:AnchorListItem ui:field="publisher" text="Publisher">
									<ui:attribute name='text' key="layout.publisher" />
								</b:AnchorListItem>
							</b:NavbarNav>
						</b:NavbarCollapse>
					</b:Column>
					<b:Column size="MD_1">
						<b:NavbarNav>
							<g:HTMLPanel ui:field="spinner" visible="false">
								<div class="{res.style.spinner}">
									<div class="{res.style.bounce1}" />
									<div class="{res.style.bounce2}" />
									<div class="{res.style.bounce3}" />
								</div>
							</g:HTMLPanel>
						</b:NavbarNav>
					</b:Column>
				</b:Row>
			</b:Container>
		</b:Navbar>
	</g:HTMLPanel>
</ui:UiBinder>
