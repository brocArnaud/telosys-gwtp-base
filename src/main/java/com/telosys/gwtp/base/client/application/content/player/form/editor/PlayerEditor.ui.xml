<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
	<b:Form>
		<b:FieldSet>
			<b:FormGroup>
				<b:FormLabel for="name" text="Name" />
				<b:InlineHelpBlock iconType="EXCLAMATION_TRIANGLE" />
				<b:TextBox b:id="name" ui:field="name" />
			</b:FormGroup>
			<b:FormGroup>
				<b:FormLabel for="team" text="Team" />
				<b:InlineHelpBlock iconType="EXCLAMATION_TRIANGLE" />
				<b:ValueListBox b:id="team" ui:field="team" />
			</b:FormGroup>
		</b:FieldSet>
	</b:Form>
</ui:UiBinder>