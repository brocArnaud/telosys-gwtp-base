<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt">
	<ui:with field="res" type="com.telosys.gwtp.base.client.resources.AppResources" />
	<g:HTMLPanel>
		<b:Container>
			<g:HTMLPanel>
				<b:Row>
					<b:Column size="MD_11" addStyleNames="{res.style.columnSmall}">
						<b:Heading size="H1" text="Author list view" />
					</b:Column>
					<b:Column size="MD_1" addStyleNames="{res.style.columnSmall}">
						<b:Button ui:field="create" text="Create" type="PRIMARY" addStyleNames="{res.style.buttonCreate}" />
					</b:Column>
				</b:Row>
				<b.gwt:CellTable ui:field="table" bordered="true" condensed="true" striped="true" hover="true" />
			</g:HTMLPanel>
		</b:Container>
	</g:HTMLPanel>
</ui:UiBinder>