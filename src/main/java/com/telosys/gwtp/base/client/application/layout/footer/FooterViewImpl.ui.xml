<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	ui:baseMessagesInterface="com.telosys.gwtp.base.client.resources.i18n.Messages">
	<ui:with field="res" type="com.telosys.gwtp.base.client.resources.AppResources" />
	<g:HTMLPanel ui:field="container">
		<b:Navbar position="FIXED_BOTTOM" addStyleNames="{res.style.footerBar}">
			<b:Container>
				<b:NavbarNav>
					<b:AnchorListItem ui:field="review" text="Review">
						<ui:attribute name='text' key="layout.review" />
					</b:AnchorListItem>
					<b:AnchorListItem ui:field="shop" text="Shop">
						<ui:attribute name='text' key="layout.shop" />
					</b:AnchorListItem>
					<b:AnchorListItem ui:field="synopsis" text="Synopsis">
						<ui:attribute name='text' key="layout.synopsis" />
					</b:AnchorListItem>
					<b:AnchorListItem ui:field="workgroup" text="Workgroup">
						<ui:attribute name='text' key="layout.workgroup" />
					</b:AnchorListItem>
				</b:NavbarNav>
				<b:NavbarText pull="RIGHT">
					<b.html:Span>
						<g:InlineLabel text="Powered by Telosys 3 -">
							<ui:attribute name='text' key="layout.powered.by" />
						</g:InlineLabel>
						<b:NavbarLink text="Visit our site for more information" href="https://github.com/telosys-tools-bricks">
							<ui:attribute name='text' key="layout.telosys.link" />
						</b:NavbarLink>
					</b.html:Span>
				</b:NavbarText>
			</b:Container>
		</b:Navbar>
	</g:HTMLPanel>
</ui:UiBinder>